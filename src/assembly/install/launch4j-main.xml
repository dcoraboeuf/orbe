<launch4jConfig>
	<headerType>gui</headerType>
	<outfile>${project.build.directory}/${project.name}-${project.version}.exe</outfile>
	<jar>${project.name}-${project.version}.jar</jar>
	<dontWrapJar>true</dontWrapJar>
	<icon>${project.build.directory}/classes/install/${project.name}.ico</icon>
	<jre>
		<minVersion>1.6.0</minVersion>
		<initialHeapSize>256</initialHeapSize>
		<maxHeapSize>1024</maxHeapSize>
	</jre>
	<versionInfo>
		<fileVersion>${orbe.version}.0</fileVersion>
		<txtFileVersion>${project.version}</txtFileVersion>
		<fileDescription>${project.description}</fileDescription>
		<copyright>Damien Coraboeuf</copyright>
		<productVersion>${orbe.version}.0</productVersion>
		<txtProductVersion>${project.version}</txtProductVersion>
		<productName>${project.name}</productName>
		<companyName>Doolin</companyName>
		<internalName>${project.name}-${project.version}</internalName>
		<originalFilename>${project.name}-${project.version}.exe</originalFilename>
	</versionInfo>
</launch4jConfig>